<div id="<%= dom_id candidate %>">
  <p>
    <strong>First name:</strong>
    <%= candidate.first_name %>
  </p>

  <p>
    <strong>Last name:</strong>
    <%= candidate.last_name %>
  </p>

  <p>
    <strong>Email:</strong>
    <%= candidate.email %>
  </p>

  <p>
    <strong>Phone:</strong>
    <%= candidate.phone %>
  </p>

  <p>
    <strong>Job code:</strong>
    <%= candidate.job_code %>
  </p>

</div>
